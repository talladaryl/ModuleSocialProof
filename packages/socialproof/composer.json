{
    "name": "packages/laravel-socialproof",
    "description": "Laravel Social Proof Package - Real-time notifications and widgets",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Packages\\SocialProof\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packages\\SocialProof\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packages\\SocialProof\\SocialProofServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}